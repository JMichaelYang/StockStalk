{
  "manifest_version": 2,

  "name": "Stalk",
  "description": "This extension provides the user access to quick stock information.",
  "version": "0.0.1",

  "content_scripts": [
      {
          "matches": ["http://*/*"],
          "js":[
              "scripts/companies.js",
              "scripts/jquery-3.2.1.min.js"
          ],
          "run_at": "document_end"
      }
  ],
  "background": {
      "scripts": [
          "scripts/background.js"
      ]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Make this page red"
  },
  "permissions": [
    "tabs",
    "<all_urls>"
  ]
}